{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "go",
    "javascript",
    "json",
    "markdown",
    "python",
    "typescript"
  ],
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh"
}
